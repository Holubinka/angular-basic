{
  "name": "angular-blog",
  "version": "0.0.0",
  "scripts": {
    "build:prod:en": "ng build --configuration=production-en",
    "build:prod:ua": "ng build --configuration=production-ua",
    "build:server:prod:en": "ng run angular-basics:server:production-en",
    "build:server:prod:ua": "ng run angular-basics:server:production-ua",
    "build:library": "ng build ngx-example-library",
    "build:client-and-server-bundles": "npm run build:prod:en && npm run build:prod:ua && npm run build:server:prod:en && npm run build:server:prod:ua",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/flex-layout": "latest",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/platform-server": "latest",
    "@angular/pwa": "^0.803.20",
    "@angular/router": "~8.2.14",
    "@angular/service-worker": "~8.2.14",
    "@nguniversal/express-engine": "latest",
    "@nguniversal/module-map-ngfactory-loader": "latest",
    "compression": "latest",
    "core-js": "^2.6.11",
    "express": "^4.17.1",
    "locale": "^0.1.0",
    "ngx-quill": "^6.3.1",
    "path": "^0.12.7",
    "quill": "^1.3.6",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "xmlhttprequest": "^1.8.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.0",
    "@angular/cli": "^8.3.20",
    "@angular/compiler-cli": "^8.1.3",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "enhanced-resolve": "^3.3.0",
    "express": "^4.15.2",
    "http-server": "^0.10.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "reflect-metadata": "^0.1.10",
    "ts-loader": "^4.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "webpack-cli": "^3.3.10"
  },
  "engines": {
    "node": "10.15.3",
    "npm": "6.9.0"
  }
}
